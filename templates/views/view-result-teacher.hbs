<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Report Of All Student</title>
    <link rel="stylesheet" href="css/app.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=IBM+Plex+Sans&display=swap"
      rel="stylesheet"
    />
  </head>
  <body>
    <div class="home">
      <a href="/" class="homepage-button">
        HOME PAGE<img
          id="homepage-icon"
          src="home.png"
          class="btn-img"
          alt=""
        /></a></a>
    </div>
    <div class="logout">
      <a href="/logout-teacher" class="logout-button">
        LOGOUT<img
          id="logout-icon"
          src="logout.png"
          class="btn-img"
          alt=""
        /></a>
      </a>
    </div>
    <div class="container">

      <table class="flex-column flex-center">
        <caption>Report Of All Students</caption>
        <tr>
          <th>Student Name</th>
          <th>Roll Number</th>
          <th>Sem & Sec</th>
          <th>Marks</th>
        </tr>
        {{#each student}}
          <div class="question-container">
            <div class="question">
              <tr>
                <td>{{fullname}}</td>
                <td>{{rno}}</td>
                <td>{{sem}}</td>
                <td>{{score}}</td>
              </tr>
            </div>
          </div>
        {{/each}}

      </table>

      <a href="/logged-in-teacher" id="back-button"><span>Go Back</span></a>
    </div>

  </body>
</html>